<%- include("partials/header") %> 
<h1>Compose</h1>
<form action="/compose" method="POST">
    <div class="form-group">
    <label for="addTitle">Title</label>
    <input type="text" name="addTitle" id="addTitle" class="form-control">
    </div>  
    <div class="form-group">
    <label for="addPost">Post</label>
    <textarea rows="5" cols="" name="addPost" id="addPost" class="form-control"></textarea>
    </div>
    <button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>
<%- include("partials/footer") %> 